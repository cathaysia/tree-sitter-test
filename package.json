{
  "name": "tree-sitter-test",
  "version": "0.1.0",
  "description": "TS corpus test grammar for tree-sitter",
  "repository": "github:tree-sitter-grammars/tree-sitter-test",
  "author": "ObserverOfTime <chronobserver@disroot.org>",
  "contributors": [
    "Amaan Qureshi <amaanq12@gmail.com>"
  ],
  "license": "MIT",
  "main": "bindings/node",
  "types": "bindings/node",
  "files": [
    "grammar.js",
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/**",
    "src/**"
  ],
  "keywords": [
    "incremental",
    "parsing",
    "tree-sitter",
    "test"
  ],
  "dependencies": {
    "node-addon-api": "^8.0.0",
    "node-gyp-build": "^4.8.0"
  },
  "devDependencies": {
    "tree-sitter-cli": "^0.22.2",
    "prebuildify": "^6.0.0"
  },
  "peerDependencies": {
    "tree-sitter": "^0.21.1"
  },
  "peerDependenciesMeta": {
    "tree_sitter": {
      "optional": true
    }
  },
  "scripts": {
    "install": "node-gyp-build",
    "prestart": "tree-sitter build --wasm -o tree-sitter-test.wasm",
    "start": "tree-sitter playground",
    "test": "node --test"
  },
  "tree-sitter": [
    {
      "scope": "text.test",
      "highlights": "queries/test/highlights.scm",
      "injections": "queries/test/injections.scm",
      "file-types": [
        "test"
      ]
    }
  ]
}
